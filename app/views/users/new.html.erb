<%= form_for(@user, url: users_path) do |p| %> 
<%= render 'shared/error_messages'%>
  <%= p.label :name %>
  <%= p.text_field :name %>

  <%= p.label :email %>
  <%= p.email_field :email %>

  <%= p.label :password %>
  <%= p.password_field :password %>

  <%= p.label :password_confirmation %>
  <%= p.password_field :password_confirmation %>
  

  <%= p.submit "Sign Up" %>

<% end %>