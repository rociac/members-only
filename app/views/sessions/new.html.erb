<% provide(:title, "Log In") %>
<div class = "container">
<div class="col-md-6 col-md-offset-3">
<%= form_for(:session) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email, class: 'form-control'  %>

  <%= f.label :password %>
  <%= f.password_field :password, class: 'form-control'  %>

  <%= f.submit "Login" %>
  
<% end %>
</div>
</div>